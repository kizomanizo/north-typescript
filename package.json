{
  "name": "thenorth-backend",
  "version": "1.0.0",
  "description": "A backend for The North credit society app.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "test": "test",
    "start": "nodemon -L",
    "start:dev": "npx prisma generate && npx prisma migrate dev --name compose && npx nodemon -L",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev --name compose",
    "debug": "nodemon --inspect=0.0.0.0:9229 index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kizomanizo/thenorth-backend.git"
  },
  "keywords": [
    "thenorth",
    "backend",
    "docker"
  ],
  "author": "Kizito Mrema",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kizomanizo/thenorth-backend/issues"
  },
  "homepage": "https://github.com/kizomanizo/thenorth-backend#readme",
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "prisma": "^5.0.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.0.4"
  }
}